{
  "name": "redux-hot-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for React/Redux project with hot code reloading",
  "scripts": {
    "start": "node server.js",
    "lint": "eslint src",
    "test": "NODE_ENV=test ava -v",
    "test:watch": "npm test -- --watch",
    "test:cov": "nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jigsawye/redux-hot-boilerplate.git"
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate",
    "webpack"
  ],
  "author": "Evan Ye <jigsaw.ye@gmail.com> (http://github.com/jigsawye)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jigsawye/redux-hot-boilerplate/issues"
  },
  "homepage": "https://github.com/jigsawye/redux-hot-boilerplate",
  "ava": {
    "files": "src/**/__tests__",
    "require": [
      "babel-core/register"
    ],
    "babel": "inherit"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-core": "^6.0.20",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "jsdom": "^8.5.0",
    "nyc": "^6.4.2",
    "react-addons-test-utils": "^15.0.2",
    "react-hot-loader": "^3.0.0-beta.1",
    "redux-logger": "^2.6.1",
    "sinon": "^1.17.4",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-actions": "^0.9.1",
    "redux-api-middleware": "^1.0.2"
  }
}
