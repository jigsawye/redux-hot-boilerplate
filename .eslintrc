{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-no-bind": 0,
    "global-require": 0,
    "comma-dangle": 1,
    "new-cap": [1, {
      "capIsNewExceptions": ["List", "Map"]
    }]
  },
  "plugins": [
    "react"
  ]
}
